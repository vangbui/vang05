@model IEnumerable<ShopGiay.Models.ReportInfo>

@{
    ViewBag.Title = "ThongKe";
    Layout = "~/Views/Shared/_LayoutU.cshtml";
}


<h2>THỐNG KÊ GIẦY THEO NHÃN HIỆU </h2>
<table border="1">
    <tr style="background-color: burlywood; font-weight: bold;">
        <td> Mã NH</td>
        <td>Số Giay </td>
        <td>Tổng Số Giay</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Count</td>
            <td>@item.Sum</td>
        </tr>
    }
</table>


