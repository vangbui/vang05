@model IEnumerable<ShopGiay.Models.NHASANXUAT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutU.cshtml";
}

<h2>Index</h2>



<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenNXB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DienThoai)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNXB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DienThoai)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MaNSX}) |
            @*@Html.ActionLink("Details", "Details", new { id = item.MaNXB }) |*@
            <a href="~/NhaXuatBan/Details?id=@item.MaNSX">Chi Tiết</a>
            @Html.ActionLink("Delete", "Delete", new { id = item.MaNSX })
        </td>
    </tr>
    }

</table>


